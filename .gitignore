# Ignore object files and executables
*.o
*.out
*.exe

# Ignore compiled dynamic libraries
*.so
*.dylib
*.dll

# Ignore precompiled headers
*.gch
*.pch

# Ignore static libraries
*.a
*.lib

# Ignore debug files
*.dSYM/
*.pdb

# Ignore generated files
*.log
*.tmp
*.bak

# Ignore directories
build/
dist/

# Ignore Visual Studio Code
.vscode/

# Ignore all files starting with 'ex'
ex*

# Allow .c files starting with 'ex'
!ex*.c

# Allow Exercises directory
!Exercises/

# Allow all .c files in Exercises directory and subdirectories
!Exercises/**/*.c
